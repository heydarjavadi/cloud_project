CREATE TABLE IF NOT EXISTS users_profile (
    id INT AUTO_INCREMENT PRIMARY KEY,
    email VARCHAR(320) NOT NULL,
    name  VARCHAR(100),   
    phoneNo INT,
    nationalCode INT,
    address VARCHAR(1000),
    postalCode INT
);
